{
  "name": "musikki-favorites-front",
  "version": "0.0.1",
  "description": "Musikki Favorites App - Artists Favorite Frontend Demo",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline --hot --port 3000 --open --history-api-fallback",
    "watch": "webpack-dev-server --hot --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/noslenfa/musikki-favorites-front"
  },
  "keywords": [
    "react",
    "reactjs",
    "musikki",
    "babel",
    "sass"
  ],
  "author": "Nelson Amaral <noslenfa@gmail.com> (http://github.com/noslenfa)",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "bootstrap": "^3.3.7",
    "css-loader": "^0.25.0",
    "node-sass": "^3.10.1",
    "react-bootstrap": "^0.30.6",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-tap-event-plugin": "^1.0.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "lodash": "^4.16.6",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^3.0.0",
    "react-router-bootstrap": "^0.23.1"
  }
}
